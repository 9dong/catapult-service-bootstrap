{
  "network": {
    "name": "mijinTest",
    "description": "catapult development network"
  },

  "port": 3000,
  "crossDomainHttpMethods": ["GET", "POST", "PUT", "OPTIONS"],
  "clientPrivateKey": "F08CE9E031B4CCD6DFEAF3D7101B6741427B6B0142FD0B62720E855469F39944",
  "extensions": ["aggregate", "multisig", "namespace", "transfer"],

  "db": {
    "url": "mongodb://db:27017/",
    "name": "catapult",
    "pageSizeMin": 10,
    "pageSizeMax": 100,
    "pageSizeStep": 25,
    "maxConnectionAttempts": 5,
    "baseRetryDelay": 500
  },

  "apiNode": {
    "host": "api-node-0",
    "port": 7900,
    "publicKey": "8674195B578501A612D9DD20DED217E037B5022E6B187BD885AD06FDC1A7979A"
  },

  "websocket": {
    "mq": {
      "host": "api-node-0",
      "port": 7902
    },
    "allowOptionalAddress": true
  },

  "logging": {
    "console": {
      "colorize": true,
      "level": "verbose",
      "handleExceptions": true,
      "prettyPrint": true,
      "timestamp": true
    },
    "file": {
      "level": "verbose",
      "handleExceptions": true,
      "prettyPrint": true,
      "timestamp": true,
      "filename": "catapult-rest.log",
      "maxsize": 20971520,
      "maxFiles": 100
    }
  }
}
